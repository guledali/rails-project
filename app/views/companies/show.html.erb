<% if @company.blank? %>
<div class="mt-10">
    <div class="container">
        <h1 class="display-4 font-weight-bold mt-15">Sorry did not found the Company!!!<h1>
    </div>
</div>
<% else %>
<div class="mt-10">
    <div class="container">
    <h1 class="display-2 font-weight-regular"> <%= @company.name %><h1>
      <div class="mt-3">
        <p>Org.nr: <%= @company.organisation_nr %></p>
        <span>Revunue <%= @company.revenue %> </span>
        <%#= params.inspect %>
      </div>
    </div>
  </div>
<% end %>

